<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="70dp">

    <ImageView
        android:id="@+id/activity_applock_item_icon"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_centerVertical="true"
        android:layout_marginLeft="20dp"
        android:src="@android:drawable/sym_def_app_icon"
        android:scaleType="fitXY"
        tools:ignore="ContentDescription" />

    <TextView
        android:id="@+id/activity_applock_item_appname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="10dp"
        tools:text="@string/app_name"
        android:textSize="@dimen/h5_12"
        android:textStyle="bold"
        android:layout_toRightOf="@id/activity_applock_item_icon"
        android:textColor="#FF333333" />

    <com.jb.filemanager.commomview.GroupSelectBox
        android:id="@+id/activity_applock_item_selectbox"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:padding="16dp" />

</RelativeLayout>